{
    "manifest_version": 3,
    "name": "前端工具集合",
    "version": "1.0",
    "description": "包含转换文件路径格式和变量命名格式的工具",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "clipboardWrite",
        "clipboardRead"
    ]
}